{
    "name": "@omm/ngrx-helpers",
    "description": "Monorepo with example app for @omm/ngrx-helpers",
    "homepage": "https://github.com/ommsolutions/ngrx-helpers",
    "repository": {
        "type": "git",
        "url": "git@github.com:ommsolutions/ngrx-helpers.git"
    },
    "license": "MIT",
    "author": "Andreas Enenkel <ae@omm-solutions.de>",
    "keywords": [
        "rxjs",
        "ngrx",
        "angular"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod --aot --sourcemap",
        "test": "ng test",
        "test:ci": "ng test --code-coverage --no-watch --config=karma.ci.conf.js",
        "coveralls": "ls -la ./ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "codacy": "ls -la ./ && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage -p .",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:lib": "ng-packagr -p src/package.json"
    },
    "dependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/cdk": "^5.2.1",
        "@angular/common": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@angular/material": "^5.2.1",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "@ngrx/effects": "^5.2.0",
        "@ngrx/entity": "^5.2.0",
        "@ngrx/router-store": "^5.2.0",
        "@ngrx/store": "^5.2.0",
        "core-js": "^2.4.1",
        "rxjs": "^5.5.8",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.3",
        "@angular/compiler": "^5.2.0",
        "@angular/compiler-cli": "^5.2.0",
        "@ngrx/store-devtools": "^5.2.0",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "^9.6.1",
        "codacy-coverage": "^2.1.0",
        "codelyzer": "^4.2.1",
        "coveralls": "^3.0.0",
        "jasmine-core": "~3.1.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^1.0.0",
        "ng-packagr": "~2.4.0",
        "ngrx-store-freeze": "^0.2.1",
        "protractor": "~5.3.0",
        "tslint": "~5.9.1",
        "typescript": "^2.6.2"
    }
}
